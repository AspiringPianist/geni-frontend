{
  "rewrites": [
    {
      "source": "/users/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/users/$1"
    },
    {
      "source": "/chats/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/chats/$1"
    },
    {
      "source": "/files/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/files/$1"
    },
    {
      "source": "/messages/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/messages/$1"
    },
    {
      "source": "/visualsummary/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/visualsummary/$1"
    },
    {
      "source": "/quiz/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/quiz/$1"
    },
    {
      "source": "/chat_with_memory/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/chat_with_memory/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://geni-backend-5l8z.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
